class Solution:
    def isPalindrome(self, x: int) -> bool:
        rev = 0
        X = x
        if (x < 0):
            return False
        while(x > 0):
            d = x % 10
            rev = rev * 10 + d
            x //= 10
        
        if (rev == X):
            return True
        else:
            return False